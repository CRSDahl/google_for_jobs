<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Jobs Configuration</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.job.renderDetailTemplate>
                        <TCEforms>
                            <label>Render Detail Template</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.job.renderDetailTemplate>
                    <settings.job.renderType>
                        <label>What to display</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0">List View</numIndex>
                                    <numIndex index="1">list</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <numIndex index="0">Detail View</numIndex>
                                    <numIndex index="1">show</numIndex>
                                </numIndex>
                            </items>
                        </config>
                    </settings.job.renderType>
                    <settings.job.detailPid>
                        <label>PageId for single job display</label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                        </config>
                    </settings.job.detailPid>
                    <settings.job.returnToPid>
                        <label>PageId to return to</label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                        </config>
                    </settings.job.returnToPid>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>